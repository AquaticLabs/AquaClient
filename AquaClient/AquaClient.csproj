        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Control.Draggable" Version="1.0.5049.269" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Resources\AquaticLabs-logo.png" />
