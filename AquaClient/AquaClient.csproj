      <None Remove="Resources\MatchLock.png" />
      <EmbeddedResource Include="Resources\MatchLock.png" />
      <None Remove="Resources\lock-logo.png" />
      <EmbeddedResource Include="Resources\lock-logo.png" />
    </ItemGroup>

</Project>